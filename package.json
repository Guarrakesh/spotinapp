{
  "name": "spotinapp",
  "version": "0.0.1",
  "private": true,
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clear": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*",
    "android-linux": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "android-dev": "ENVFILE=.env.dev react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "android-prod": "ENVFILE=.env.prod react-native run-android",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios --scheme 'Spotin Dev'",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme 'Spotin Staging'",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --scheme 'Spotin Prod'",
    "build-android-prod": "export ENVFILE=.env.prod && cd android && ./gradlew assembleRelease && cd ..",
    "build-android-staging": "export ENVFILE=.env.staging && cd android && ./gradlew assembleReleaseStaging && cd .."
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.3.3",
    "appcenter": "^1.9.0",
    "appcenter-analytics": "^1.9.0",
    "appcenter-crashes": "^1.9.0",
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "d3-interpolate": "^1.3.2",
    "i18next": "^12.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "16.4.1",
    "react-i18next": "^8.3.5",
    "react-native": "^0.56.0",
    "react-native-action-button": "^2.8.5",
    "react-native-android-location-enabler": "^1.0.8",
    "react-native-android-open-settings": "^1.2.0",
    "react-native-animatable": "^1.3.0",
    "react-native-app-intro": "^1.1.5",
    "react-native-app-intro-slider": "^1.0.1",
    "react-native-cli": "^2.0.1",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.5",
    "react-native-content-loader": "^0.1.6",
    "react-native-device-info": "^0.24.3",
    "react-native-easy-grid": "^0.2.0",
    "react-native-elements": "^1.0.0-beta6",
    "react-native-fbsdk": "^0.8.0",
    "react-native-geolocation-service": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.3",
    "react-native-maps": "^0.21.0",
    "react-native-modal": "^6.5.0",
    "react-native-permissions": "^1.1.1",
    "react-native-phone-call": "^1.0.8",
    "react-native-snap-carousel": "^3.7.4",
    "react-native-svg": "^7.0.3",
    "react-native-swiper": "^1.5.13",
    "react-native-tab-view": "^1.2.0",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.6.0",
    "react-navigation-fluid-transitions": "^0.2.2",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sinon": "^6.1.3",
    "stringify": "^5.2.0",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "babel-preset-react-native": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "jest": "22.4.4",
    "react-test-renderer": "16.3.1",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
